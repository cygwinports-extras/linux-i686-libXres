CROSS_HOST="i686-pc-linux-gnu"
ORIG_PN="libXres"
inherit cross xorg

NAME="linux-i686-libXres"
VERSION=1.0.6
RELEASE=1
CATEGORY="Devel"
SUMMARY="X.Org XResource library for Linux i686 toolchain"
